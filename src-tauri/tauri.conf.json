{
  "productName": "Lightning Timer",
  "version": "0.1.0",
  "identifier": "com.lightning-timer.dev",
  "build": {
    "beforeDevCommand": "npm run dev",
    "beforeBuildCommand": "npm run build",
    "frontendDist": "../dist"
  },
  "app": {
    "windows": [
      {
        "label": "main",
        "title": "Lightning Timer",
        "width": 800,
        "height": 200,
        "minWidth": 800,
        "minHeight": 200,
        "maxWidth": 800,
        "maxHeight": 200,
        "resizable": false,
        "alwaysOnTop": false,
        "decorations": true,
        "transparent": false,
        "visible": true,
        "center": true,
        "devtools": true
      },
      {
        "label": "numberpad",
        "title": "Number Pad",
        "width": 300,
        "height": 400,
        "minWidth": 300,
        "minHeight": 400,
        "maxWidth": 300,
        "maxHeight": 400,
        "resizable": false,
        "alwaysOnTop": true,
        "decorations": true,
        "transparent": false,
        "visible": false,
        "center": true,
        "url": "numberpad.html"
      }
    ]
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  }
}